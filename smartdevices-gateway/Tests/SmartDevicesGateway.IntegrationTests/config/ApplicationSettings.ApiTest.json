{
  "ServiceName": "SmartDevicesGatewayTest",
  "AmqpServiceConfig": {
    "ConnectionString": "amqp://user:password@localhost:5672",
    "ClientName": "SmartDevicesGatewayTest",
    "QueueName": "sdgw"
  },
  "ServerUrls": [
    "http://*:7000"
  ],
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Debug"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Debug"
      }
    }
  },
  "Logging.Development": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": null,
    "ClientIdHeader": null,
    "HttpStatusCode": 429,
    "IpWhitelist": [],
    "EndpointWhitelist": [],
    "ClientWhitelist": [],
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1m",
        "Limit": 20000
      }
    ]
  },
  "AmqpOption": {
    "UseAmqp": false
  }
}